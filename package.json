{
  "name": "deel-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/express": "^4.17.15",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "8.8.0",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^4.0.1",
    "typedi": "^0.10.0"
  },
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node ./src/server.ts\"",
    "lint": "tsc --noEmit && eslint .",
    "lint:code": "eslint .",
    "lint:fix": "tsc --noEmit && eslint . --fix",
    "migrate": "yarn run clean && yarn run build && env-cmd -f .env sequelize db:migrate",
    "migrate:undo": "yarn run clean && yarn run build && env-cmd -f .env sequelize db:migrate:undo",
    "migration:generate": "sequelize migration:generate --migrations-path src/migrations --name",
    "seed": "yarn run clean && yarn run build && env-cmd -f .env sequelize db:seed:all",
    "build": "tsc -p .",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.40.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "engineStrict": true
}
